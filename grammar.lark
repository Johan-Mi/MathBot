?start			: expr
?expr			: addition
?addition		: expr "+" expr	-> _add
				| expr "-" expr	-> _sub
				| multiplication
?multiplication	: expr "*" expr	-> _mul
				| expr "/" expr	-> _div
				| power
?power			: expr "^" expr	-> _exp
				| atom
?atom			: "(" expr ")"
				| NUMBER		-> _num
				| "-" expr		-> _negate

%import common.NUMBER
%import common.WS
%ignore WS
